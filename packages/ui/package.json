{
  "name": "@acme/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "exports": {
    ".": "./src/index.tsx"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "upgrade": "pnpm upgrade --latest",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "build": "tamagui-build",
    "watch": "tamagui-build --watch"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "7.6.1",
    "@shopify/flash-list": "1.6.3",
    "@tamagui/animations-moti": "^1.90.6",
    "@tamagui/font-inter": "^1.90.6",
    "@tamagui/react-native-media-driver": "^1.90.6",
    "@tamagui/shorthands": "^1.90.6",
    "@tamagui/themes": "^1.90.6",
    "@tamagui/toast": "^1.90.6",
    "@tanstack/react-virtual": "^3.4.0",
    "expo-blur": "~12.9.2",
    "tamagui": "^1.90.6"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@tamagui/build": "^1.90.6",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
{
  "name": "@rooots/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "main": "src/index.ts",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "upgrade": "pnpm upgrade --latest",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "watch": "tamagui-build --watch"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.3.0",
    "@shopify/flash-list": "1.7.6",
    "@tamagui/animations-moti": "^1.128.1",
    "@tamagui/font-inter": "^1.128.1",
    "@tamagui/react-native-media-driver": "^1.128.1",
    "@tamagui/shorthands": "^1.128.1",
    "@tamagui/themes": "^1.128.1",
    "@tamagui/toast": "^1.128.1",
    "@tanstack/react-virtual": "^3.4.0",
    "expo-blur": "~14.1.4",
    "tamagui": "^1.128.1"
  },
  "devDependencies": {
    "@tamagui/build": "^1.128.1",
    "typescript": "^5.3.3"
  }
}
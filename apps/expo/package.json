{
  "name": "@acme/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --dev-client --ios",
    "clean": "git clean -xdf .expo .turbo node_modules .tamagui",
    "upgrade": "pnpm upgrade --latest",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "log:android": "npx react-native log-android",
    "log:ios": "npx react-native log-ios",
    "build:android": "eas build --profile preview --platform android",
    "build:android:local": "eas build --profile preview --platform android --local",
    "build:ios": "eas build --profile development --platform ios"
  },
  "dependencies": {
    "@acme/app": "workspace:^0.0.0",
    "@acme/ui": "workspace:*",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/query-async-storage-persister": "^5.28.6",
    "@tanstack/react-query": "^5.39.0",
    "@tanstack/react-query-persist-client": "^5.28.6",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "babel-plugin-inline-import": "^3.0.0",
    "expo": "^51.0.8",
    "expo-blur": "~13.0.2",
    "expo-constants": "^16.0.1",
    "expo-dev-client": "~4.0.14",
    "expo-linking": "^6.3.1",
    "expo-router": "^3.5.14",
    "expo-splash-screen": "^0.27.4",
    "expo-sqlite": "~14.0.3",
    "expo-status-bar": "^1.12.1",
    "jotai": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "~4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.2.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@acme/api": "workspace:^0.1.0",
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@expo/config-plugins": "^8.0.0",
    "@expo/metro-config": "^0.18.4",
    "@expo/ngrok": "^4.1.3",
    "@tamagui/babel-plugin": "^1.90.6",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.58",
    "babel-preset-expo": "^11.0.6",
    "eslint": "^8.57.0",
    "metro-minify-terser": "^0.80.8",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ],
    "ignorePatterns": [
      "expo-plugins/**"
    ]
  },
  "prettier": "@acme/prettier-config"
}